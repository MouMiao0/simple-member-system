{
  "name": "member-system",
  "version": "2.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Really simple Electron + Vue + Vite boilerplate.",
  "author": "某涛喵 <1606958950@qq.com>",
  "license": "MTM",
  "private": true,
  "scripts": {
    "dev": "chcp 65001 && vue-tsc --noEmit && vite ",
    "build": "vue-tsc --noEmit && vite build && electron-builder install-app-deps && electron-builder",
    "electron:rebuild": "electron-rebuild -f -w sqlite3",
    "postinstall": "electron-builder install-app-deps"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@element-plus/icons-vue": "^2.0.10",
    "element-plus": "^2.2.25",
    "fs-extra": "^11.1.0",
    "pg-hstore": "^2.3.4",
    "pinia": "^2.0.27",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.26.0",
    "sqlite3": "^5.1.2",
    "ts-node": "^10.9.1",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.10",
    "@types/validator": "^13.7.10",
    "@vitejs/plugin-vue": "^3.1.2",
    "electron": "^21.1.0",
    "electron-builder": "^23.3.3",
    "electron-rebuild": "^3.2.9",
    "typescript": "^4.9.3",
    "vite": "^3.2.2",
    "vite-plugin-electron": "^0.10.4",
    "vite-plugin-electron-renderer": "^0.11.3",
    "vite-plugin-top-level-await": "^1.2.2",
    "vue": "^3.2.40",
    "vue-tsc": "^1.0.9",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ]
}
